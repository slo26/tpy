<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
        xsi:noNamespaceSchemaLocation="https://docs.phpdoc.org/latest/phpdoc.xsd"
>
    <paths>
            <output>web/docs/api</output>
            <cache>web/docs/cache</cache>
    </paths>
    <version number="latest">
        <api>
            <source dsn="/projects/drup/base/tpy/web">
                <path>modules/custom</path>
            </source>
            <extensions>
                <extension>php</extension>
                <extension>module</extension>
            </extensions>
            <ignore hidden="true" symlinks="true">
                <path>modules/custom/billing_write_off/src/Controller/**/*</path>
                <path>modules/custom/ckeditor_moverow/**/*</path>
                <path>modules/custom/common_utils/src/Form/**/*</path>
                <path>modules/custom/custom_toolbar/**/*</path>
                <path>modules/custom/**/*/src/Form/**/*</path>
                <path>modules/custom/**/*/src/Controller/**/*</path>
            </ignore>
        </api>
    </version>

</phpdocumentor>
